                            <input type="text" name="rolename" value="{{nombre}}" id="rolename" class="form-control border-dark border-2 rounded-end" required>
                            <div class="valid-feedback">
                                ¡Perfecto!
                            </div>
                            <div class="invalid-feedback">
                                <span id="invalid-feedback-rolename"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row py-2 px-0">
                        <label for="rolestatusop1" class="form-label">Estatus:</label>
                        <div class="row m-0 justify-content-md-start">
                            <div class="col-6 d-grid ps-0">
                                <input type="radio" name="rolestatus" id="rolestatusop1" class="btn-check" value="1" autocomplete="off" {{#if activo}}checked{{/if}}>
                                <label for="rolestatusop1" class="btn btn-outline-success border-success border-2">Activo</label>
                            </div>
                            <div class="col-6 d-grid pe-0">
                                <input type="radio" name="rolestatus" id="rolestatusop2" class="btn-check" value="0" autocomplete="off" {{#unless activo}}checked{{/unless}}>
                                <label for="rolestatusop2" class="btn btn-outline-danger border-danger border-2">Inactivo</label>
                            </div>
                        </div>
                    </div>
                    <div class="row py-2 px-0">
                        <label for="roledescription" class="form-label">Descripción:</label>
                        <div class="input-group has-validation">
                            <span class="input-group-text border-dark border-2 border-end-0"><i class="bi bi-info-square"></i></span>
                            <textarea class="form-control border-dark border-2 rounded-end" name="roledescription" id="roledescription" rows="6" required>{{descripcion}}</textarea>
                            <div class="valid-feedback">
                                ¡Perfecto!
                            </div>
                            <div class="invalid-feedback">
